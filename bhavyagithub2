#include<stdio.h>

void main()

{

  int n,i,sum=0;

  printf("Enter the value of n\n");

  scanf("%d",&n);

  for(i=0;i<n;i++)

  {

    if(i%3==0)

    {

      sum=sum+i;

    }

  }

    printf("sum=%d\n",sum);

}

